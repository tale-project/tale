    "test:coverage": "vitest --coverage --project server",
    "test:ui": "vitest --run --config vitest.ui.config.mjs",
    "test:ui:watch": "vitest --config vitest.ui.config.mjs",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build -o storybook-static",
    "stress-test": "npx tsx stress-tests/run-stress-test.ts",
    "stress-test:concurrent": "npx tsx stress-tests/scenarios/concurrent-starts.ts",
    "stress-test:sustained": "npx tsx stress-tests/scenarios/sustained-load.ts",
