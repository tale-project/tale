    "setup": "echo 'Dependencies managed by root workspace'",
    "dev": "node scripts/dev.mjs",
    "build": "vite build",
    "start": "vite preview",
    "lint": "oxlint --type-aware",
    "lint:fix": "oxlint --type-aware --fix",
    "format": "oxfmt -c ../../.oxfmtrc.json",
    "format:check": "oxfmt -c ../../.oxfmtrc.json --check",
    "typecheck": "tsc --noEmit",
    "convex:dev": "CONVEX_AGENT_MODE=anonymous convex dev",
    "convex:deploy": "CONVEX_AGENT_MODE=anonymous convex deploy",
